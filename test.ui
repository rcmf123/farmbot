<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="main">
    <property name="borderwidth">5</property>
    <property name="height">200</property>
    <property name="padding">10</property>
    <property name="width">1000</property>
    <layout>
      <property name="propagate">True</property>
    </layout>
    <child>
      <object class="ttk.Label" id="Label_farmbot">
        <property name="cursor">based_arrow_down</property>
        <property name="foreground">#ff0000</property>
        <property name="padding">10</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">FARMBOT</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="quit_button">
        <property name="command">on_quit_button_click</property>
        <property name="compound">top</property>
        <property name="cursor">arrow</property>
        <property name="state">normal</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Quit</property>
        <layout>
          <property name="column">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">se</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_map">
        <property name="text" translatable="yes">Visual Map</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Button_map">
        <property name="command">button_map</property>
        <property name="text" translatable="yes">Show visual map</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
